<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lxy.A01.mapper.Test1Mapper">

    <!--
        resultType 返回值类型
            查询返回的时候一般不是对象就是接口
            如果返回值是一个对象，就写对象所在包的路径
            如果返回值是一个集合，就写入集合的泛型


    -->
    <select id="selectTest1" resultType="com.lxy.pojo.Test1">
        select * from test1
    </select>

    <select id="selectTest2" resultType="test1">
        select * from test1
    </select>


</mapper>